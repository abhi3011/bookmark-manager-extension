{
  "manifest_version": 3,
  "name": "Bookmark manager",
  "version": "0.1",

  "action": {
    "default_icon": {
      "16": "logo.png",
      "32": "logo.png",
      "64": "logo.png"
    },
    "default_popup": "popup.html"
  },
  "description": "A chrome extension to manage your bookmarks",

  "permissions": ["tabs", "background", "storage"],

  "author": "Abhilash"
}
